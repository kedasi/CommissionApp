<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("ArtExamples", "Index", "ArtExamples")</li>
                    <li>@Html.ActionLink("ArtExampleTags", "Index", "ArtExampleTags")</li>
                    <li>@Html.ActionLink("ArtMessages", "Index", "ArtMessages")</li>
                    <li>@Html.ActionLink("CreditCardInfos", "Index", "CreditCardInfos")</li>
                    <li>@Html.ActionLink("CreditCardUsers", "Index", "CreditCardUsers")</li>
                    <li>@Html.ActionLink("Pictures", "Index", "Pictures")</li>
                    <li>@Html.ActionLink("Projects", "Index", "Projects")</li>
                    <li>@Html.ActionLink("ProjectStatusReports", "Index", "ProjectStatusReports")</li>
                    <li>@Html.ActionLink("ProjectStatusReportComments", "Index", "ProjectStatusReportComments")</li>
                    <li>@Html.ActionLink("Tags", "Index", "Tags")</li>
                    <li>@Html.ActionLink("TagTypes", "Index", "TagTypes")</li>
                    <li>@Html.ActionLink("Users", "Index", "Users")</li>
                    <li>@Html.ActionLink("UserFeedbacks", "Index", "UserFeedbacks")</li>
                    <li>@Html.ActionLink("UserMessages", "Index", "UserMessages")</li>
                    <li>@Html.ActionLink("UserTags", "Index", "UserTags")</li>
                    <li>@Html.ActionLink("UserTypes", "Index", "UserTypes")</li>
                    <
                    
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
